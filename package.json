{
  "name": "danchi-site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "APP_ENV=dev run-p dev:*",
    "dev-local": "APP_ENV=local run-p dev:*",
    "dev:next": "next dev",
    "dev:path": "pathpida --ignorePath .gitignore --watch",
    "dev:open": "open http://localhost:3000",
    "build": "pathpida --ignorePath .gitignore && export APP_ENV=dev && next build",
    "export": "next export",
    "start": "next start",
    "lint": "next lint",
    "s3:cp": "aws s3 cp ./out s3://danchi-web-site-test --recursive",
    "s3:view": "open http://danchi-web-site-test.s3-website-ap-northeast-1.amazonaws.com/",
    "aspida:make": "openapi2aspida",
    "aspida:remake": "rm -rf src/api/* && openapi2aspida",
    "deploy:test": "npm run build && npm run export && npm run s3:cp && npm run s3:view"
  },
  "dependencies": {
    "@aspida/fetch": "^1.9.1",
    "@aspida/swr": "^1.9.1",
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "@mui/material": "^5.5.3",
    "@tailwindcss/line-clamp": "^0.3.1",
    "aspida": "^1.9.1",
    "microcms-js-sdk": "^2.0.0",
    "next": "12.1.1",
    "next-ts-tailwind": "^1.1.2-2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.29.0",
    "react-icons": "^4.3.1",
    "soundcloud-widget": "^0.2.1",
    "swr": "^1.3.0"
  },
  "devDependencies": {
    "@types/node": "17.0.23",
    "@types/react": "17.0.43",
    "@types/react-dom": "17.0.14",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "autoprefixer": "^10.4.4",
    "eslint": ">=5.16.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-next": "12.1.1",
    "eslint-plugin-react": "^7.29.4",
    "npm-run-all": "^4.1.5",
    "openapi2aspida": "^0.19.0",
    "pathpida": "^0.18.0",
    "postcss": "^8.4.12",
    "prettier": "^2.8.1",
    "tailwindcss": "^3.0.23",
    "ts-node": "^10.7.0",
    "typescript": "4.6.3"
  }
}
